    public boolean isPalindrome(int x) {
        int temp = x;
        if (temp < 0)
            return false;
        int result = 0;
        while (temp > 0){
            result = result * 10 + temp % 10;
            temp /= 10;
        }
        if (result == x)
            return true;
        else
            return false;
    }
